alias ssha='eval $(ssh-agent) && ssh-add'
alias dcfr='docker-compose up -d --force-recreate'
alias dcu='docker-compose up -d'
alias ls='ls -l'
alias vim='nvim'
alias ..="cd .."
alias whatup="sudo systemctl status"
alias bat="batcat"
alias pyans='source /home/scott/ansible/bin/activate'

