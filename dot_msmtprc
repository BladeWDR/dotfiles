account scott@ssb-tech.net
host mail.ssb-tech.net
port 465
from scott@ssb-tech.net
user scott@ssb-tech.net
passwordeval "pass scott@ssb-tech.net"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/scott/.local/state/msmtp/msmtp.log
tls_starttls off
